{"ast":null,"code":"import request from \"@/utils/request\";\nimport Cookies from \"js-cookie\";\nexport default {\n  name: 'BorrowList',\n  data() {\n    return {\n      admin: Cookies.get('admin') ? JSON.parse(Cookies.get('admin')) : {},\n      tableData: [],\n      total: 0,\n      params: {\n        pageNumber: 1,\n        pageSize: 10,\n        name: '',\n        book_id: ''\n      }\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      // fetch('http://localhost:9090/user/list').then(res=>res.json()).then(res=>{\n      //     console.log(res)\n      //     this.tableData=res\n      // })\n      request.get('/borrow/page', {\n        params: this.params\n      }).then(res => {\n        if (res.code == '200') {\n          this.tableData = res.data.list;\n          this.total = res.data.total;\n        }\n      });\n    },\n    reset() {\n      this.params = {\n        pageNumber: 1,\n        pageSize: 10,\n        name: '',\n        book_id: ''\n      };\n      this.load();\n    },\n    del(id) {\n      request.delete(\"/borrow/delete/\" + id).then(res => {\n        if (res.code == '200') {\n          this.$notify.success(\"删除成功\");\n          this.load();\n        } else this.$notify.error(res.msg);\n      });\n    },\n    //点击切换分页\n    handleCurrentChanged(pageNumber) {\n      this.params.pageNumber = pageNumber;\n      this.load();\n    },\n    returnBook(row) {\n      request.delete(\"/returns/deleteReturns/\" + id).then(res => {\n        if (res.code == '200') {\n          this.$notify.success(\"删除成功\");\n          this.load();\n        } else this.$notify.error(res.msg);\n      });\n    }\n  }\n};","map":{"version":3,"names":["request","Cookies","name","data","admin","get","JSON","parse","tableData","total","params","pageNumber","pageSize","book_id","created","load","methods","then","res","code","list","reset","del","id","delete","$notify","success","error","msg","handleCurrentChanged","returnBook","row"],"sources":["src/views/borrow/List.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <!--      搜索表单-->\r\n        <div style=\"margin-bottom: 35px;margin-top: 30px\">\r\n            <el-input style=\"width:240px\" placeholder=\"请输入书名\" v-model=\"params.bookName\"> </el-input>\r\n            <el-input style=\"width:240px;margin-left:5px\" placeholder=\"请输入图书编号\" v-model=\"params.bookNo\"> </el-input>\r\n            <el-input style=\"width:240px;margin-left:5px\" placeholder=\"请输入用户名\" v-model=\"params.userName\"> </el-input>\r\n            <el-button style=\"margin-left:5px\" type=\"primary\"@click=\"load\"><i class=\"el-icon-search\"></i> 搜索</el-button>\r\n            <el-button style=\"margin-left:5px\" type=\"warning\"@click=\"reset\"><i class=\"el-icon-refresh\"></i> 重置</el-button>\r\n        </div>\r\n\r\n\r\n        <el-table :data=\"tableData\" stripe>\r\n            <el-table-column prop=\"id\" label=\"编号\" width=\"50\"></el-table-column>\r\n<!--            <el-table-column prop=\"cover\" label=\"封面\">-->\r\n<!--                <meta name=\"referrer\" content=\"no-referrer\" />-->\r\n<!--                <template v-slot=\"scope\">-->\r\n<!--                    <el-image :src=\"scope.row.cover\" :preview-src-list=\"[scope.row.cover]\"></el-image>-->\r\n<!--                </template>-->\r\n<!--            </el-table-column>-->\r\n            <el-table-column prop=\"bookName\" label=\"书名\" width=\"90\"></el-table-column>\r\n            <el-table-column prop=\"bookNo\" label=\"图书编号\" width=\"120\"></el-table-column>\r\n            <el-table-column prop=\"userName\" label=\"用户姓名\" width=\"70\"></el-table-column>\r\n            <el-table-column prop=\"userId\" label=\"用户编号\" width=\"130\"></el-table-column>\r\n            <el-table-column prop=\"userPhone\" label=\"用户联系电话\" width=\"110\"></el-table-column>\r\n            <el-table-column prop=\"score\" label=\"花费积分\" width=\"70\"></el-table-column>\r\n            <el-table-column prop=\"status\" label=\"借出状态\" width=\"70\"></el-table-column>\r\n            <el-table-column prop=\"createtime\" label=\"借书时间\" width=\"90\"></el-table-column>\r\n            <el-table-column prop=\"days\" label=\"借出天数\" width=\"70\"></el-table-column>\r\n            <el-table-column prop=\"returnDate\" label=\"归还日期\" width=\"90\"></el-table-column>\r\n            <el-table-column prop=\"note\" label=\"过期提醒\" width=\"90\">\r\n            <template v-slot=\"scope\">\r\n                <el-tag type=\"success\" v-if=\"scope.row.note==='正常'\">{{scope.row.note}}</el-tag>\r\n                <el-tag type=\"primary\"v-if=\"scope.row.note==='即将到期'\">{{scope.row.note}}</el-tag>\r\n                <el-tag type=\"warning\"v-if=\"scope.row.note==='已到期'\">{{scope.row.note}}</el-tag>\r\n                <el-tag type=\"danger\"v-if=\"scope.row.note==='已过期'\">{{scope.row.note}}</el-tag>\r\n            </template>\r\n            </el-table-column>\r\n            <el-table-column  label=\"管理\" width=\"70\">\r\n                <template v-slot=\"scope\">\r\n                    <el-button type=\"primary\" @click=\"returnBook\">还书</el-button>\r\n                </template>\r\n            </el-table-column>\r\n\r\n            <el-table-column  label=\"操作\" >\r\n                <template v-slot=\"scope\">\r\n<!--                    scope.row 就是当前行数据-->\r\n                    <div style=\"display: flex\">\r\n                    <el-popconfirm\r\n                            style=\"padding-left: 2px\"\r\n                            confirm-button-text='YES'\r\n                            cancel-button-text='NO'\r\n                            icon=\"el-icon-info\"\r\n                            title=\"是否确定删除该行数据\"\r\n                            @confirm=\"del(scope.row.id)\">\r\n                    <el-button  type=\"danger\" slot=\"reference\">删除</el-button>\r\n                    </el-popconfirm>\r\n                    </div>\r\n                </template>\r\n            </el-table-column>\r\n\r\n        </el-table>\r\n        <!--      分页-->\r\n        <div style=\"margin-top: 20px\">\r\n            <el-pagination\r\n                    background\r\n                    :current=\"params.pageNumber\"\r\n                    :page-size=\"params.pageSize\"\r\n                    layout=\"prev, pager, next\"\r\n                    @current-change=\"handleCurrentChanged\"\r\n                    :total=\"total\">\r\n            </el-pagination>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport request from \"@/utils/request\";\r\nimport Cookies from \"js-cookie\";\r\n\r\nexport default {\r\n    name: 'BorrowList',\r\n    data(){\r\n        return {\r\n            admin: Cookies.get('admin') ? JSON.parse(Cookies.get('admin')) : {},\r\n            tableData: [],\r\n            total:0,\r\n            params:{\r\n                pageNumber:1,\r\n                pageSize:10,\r\n                name:'',\r\n                book_id:''\r\n            }\r\n        }\r\n    },\r\n    created() {\r\n        this.load()\r\n    },\r\n    methods:{\r\n        load(){\r\n            // fetch('http://localhost:9090/user/list').then(res=>res.json()).then(res=>{\r\n            //     console.log(res)\r\n            //     this.tableData=res\r\n            // })\r\n            request.get('/borrow/page',{\r\n                params: this.params\r\n            }).then(res=>{\r\n                if(res.code=='200') {\r\n                    this.tableData = res.data.list\r\n                    this.total = res.data.total\r\n                }\r\n            })\r\n        },\r\n        reset(){\r\n            this.params = {\r\n                pageNumber:1,\r\n                pageSize:10,\r\n                name:'',\r\n                book_id:''\r\n            }\r\n            this.load()\r\n        },\r\n        del(id){\r\n            request.delete(\"/borrow/delete/\"+id).then(res=>{\r\n                if(res.code=='200') {\r\n                    this.$notify.success(\"删除成功\")\r\n                    this.load()\r\n                }\r\n                else\r\n                    this.$notify.error(res.msg)\r\n            });\r\n\r\n        },\r\n        //点击切换分页\r\n        handleCurrentChanged(pageNumber){\r\n            this.params.pageNumber=pageNumber\r\n            this.load()\r\n        },\r\n        returnBook(row){\r\n            request.delete(\"/returns/deleteReturns/\"+id).then(res=>{\r\n                if(res.code=='200') {\r\n                    this.$notify.success(\"删除成功\")\r\n                    this.load()\r\n                }\r\n                else\r\n                    this.$notify.error(res.msg)\r\n            });\r\n\r\n        }\r\n    }\r\n}\r\n</script>\r\n"],"mappings":"AA8EA,OAAAA,OAAA;AACA,OAAAC,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,KAAA,EAAAH,OAAA,CAAAI,GAAA,YAAAC,IAAA,CAAAC,KAAA,CAAAN,OAAA,CAAAI,GAAA;MACAG,SAAA;MACAC,KAAA;MACAC,MAAA;QACAC,UAAA;QACAC,QAAA;QACAV,IAAA;QACAW,OAAA;MACA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAD,KAAA;MACA;MACA;MACA;MACA;MACAf,OAAA,CAAAK,GAAA;QACAK,MAAA,OAAAA;MACA,GAAAO,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAX,SAAA,GAAAU,GAAA,CAAAf,IAAA,CAAAiB,IAAA;UACA,KAAAX,KAAA,GAAAS,GAAA,CAAAf,IAAA,CAAAM,KAAA;QACA;MACA;IACA;IACAY,MAAA;MACA,KAAAX,MAAA;QACAC,UAAA;QACAC,QAAA;QACAV,IAAA;QACAW,OAAA;MACA;MACA,KAAAE,IAAA;IACA;IACAO,IAAAC,EAAA;MACAvB,OAAA,CAAAwB,MAAA,qBAAAD,EAAA,EAAAN,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAM,OAAA,CAAAC,OAAA;UACA,KAAAX,IAAA;QACA,OAEA,KAAAU,OAAA,CAAAE,KAAA,CAAAT,GAAA,CAAAU,GAAA;MACA;IAEA;IACA;IACAC,qBAAAlB,UAAA;MACA,KAAAD,MAAA,CAAAC,UAAA,GAAAA,UAAA;MACA,KAAAI,IAAA;IACA;IACAe,WAAAC,GAAA;MACA/B,OAAA,CAAAwB,MAAA,6BAAAD,EAAA,EAAAN,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAM,OAAA,CAAAC,OAAA;UACA,KAAAX,IAAA;QACA,OAEA,KAAAU,OAAA,CAAAE,KAAA,CAAAT,GAAA,CAAAU,GAAA;MACA;IAEA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}