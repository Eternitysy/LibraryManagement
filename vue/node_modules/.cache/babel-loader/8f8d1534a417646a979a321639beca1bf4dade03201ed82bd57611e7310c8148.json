{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport request from \"@/utils/request\";\nexport default {\n  name: \"EditBorrow\",\n  data() {\n    const checkPhone = (rule, value, callback) => {\n      if (!/^[1][3,4,5,6,7,8,9][0-9]{9}$/.test(value)) {\n        callback(new Error('该手机号不合法'));\n      }\n      callback();\n    };\n    return {\n      form: {},\n      rules: {\n        name: [{\n          required: true,\n          message: '请输入书名',\n          trigger: 'blur'\n        }],\n        phone: [{\n          validator: checkPhone,\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  methods: {\n    save() {\n      request.put('/borrow/update', this.form).then(res => {\n        if (res.code == '200') {\n          this.$notify.success('修改成功');\n          this.$router.push(\"/borrowList\");\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n    selBook() {\n      const book = this.books.find(v => v.bookNo = this.form.bookNo);\n      request.get('/book/' + book.id).then(res => {\n        this.form.bookName = res.data.name;\n        this.form.score = res.data.score;\n        this.form.nums = res.data.nums;\n      });\n    },\n    selUser() {\n      const user = this.users.find(v => v.cardID = this.form.userId);\n      request.get('/user/' + user.id).then(res => {\n        this.form.userName = res.data.name;\n        this.form.userPhone = res.data.phone;\n        this.form.score = res.data.account;\n      });\n    }\n  }\n};","map":{"version":3,"names":["request","name","data","checkPhone","rule","value","callback","test","Error","form","rules","required","message","trigger","phone","validator","methods","save","put","then","res","code","$notify","success","$router","push","error","msg","selBook","book","books","find","v","bookNo","get","id","bookName","score","nums","selUser","user","users","cardID","userId","userName","userPhone","account"],"sources":["src/views/borrow/Edit.vue"],"sourcesConten