{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport request from \"@/utils/request\";\nimport Cookies from 'js-cookie';\nexport default {\n  name: 'AdminList',\n  data() {\n    return {\n      admin: Cookies.get('admin') ? JSON.parse(Cookies.get('admin')) : {},\n      tableData: [],\n      total: 0,\n      form: {},\n      dialogFormVisible: false,\n      params: {\n        pageNumber: 1,\n        pageSize: 10,\n        username: '',\n        phone: '',\n        email: ''\n      },\n      rules: {\n        newPass: [{\n          required: true,\n          message: '请输入新密码',\n          trigger: 'blur'\n        }, {\n          min: 3,\n          max: 10,\n          message: '长度在3-10个字符',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      // fetch('http://localhost:9090/admin/list').then(res=>res.json()).then(res=>{\n      //     console.log(res)\n      //     this.tableData=res\n      // })\n      request.get('/admin/page', {\n        params: this.params\n      }).then(res => {\n        if (res.code == '200') {\n          this.tableData = res.data.list;\n          this.total = res.data.total;\n        }\n      });\n    },\n    reset() {\n      this.params = {\n        pageNumber: 1,\n        pageSize: 10,\n        username: '',\n        phone: '',\n        email: ''\n      };\n      this.load();\n    },\n    del(id) {\n      request.delete(\"/admin/delete/\" + id).then(res => {\n        if (res.code == '200') {\n          this.$notify.success(\"删除成功\");\n          this.load();\n        } else this.$notify.error(res.msg);\n      });\n    },\n    //点击切换分页\n    handleCurrentChanged(pageNumber) {\n      this.params.pageNumber = pageNumber;\n      this.load();\n    },\n    handleChangePass(row) {\n      this.dialogFormVisible = true;\n      this.form = JSON.parse(JSON.stringify(row));\n    },\n    savePass() {\n      this.$refs[\"formRef\"].validate(valid => {\n        if (valid) {\n          request.put('/admin/password', this.form).then(res => {\n            if (res.code === '200') {\n              this.$notify.success(\"修改成功\");\n              if (this.form.id === this.admin.id) {\n                //当前修改的用户id等于当前登录的管理员id，修改成功后需要重新登录\n                Cookies.remove('admin');\n                this.$router.push('/login');\n              } else {\n                this.load();\n                this.dialogFormVisible = false;\n              }\n            } else {\n              this.$notify.error(\"修改失败\");\n            }\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["request","Cookies","name","data","admin","get","JSON","parse","tableData","total","form","dialogFormVisible","params","pageNumber","pageSize","username","phone","email","rules","newPass","required","message","trigger","min","max","created","load","methods","then","res","code","list","reset","del","id","delete","$notify","success","error","msg","handleCurrentChanged","handleChangePass","row","stringify","savePass","$refs","validate","valid","put","remove","$router","push"],"sources":["src/views/admin/List.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <!--      搜索表单-->\r\n        <div style=\"margin-bottom: 35px;margin-top: 30px\">\r\n            <el-input style=\"width:240px\" placeholder=\"请输入名称\" v-model=\"params.name\"> </el-input>\r\n            <el-input style=\"width:240px;margin-left:5px\" placeholder=\"请输入联系方式\" v-model=\"params.phone\"> </el-input>\r\n            <el-button style=\"margin-left:5px\" type=\"primary\"@click=\"load\"><i class=\"el-icon-search\"></i> 搜索</el-button>\r\n            <el-button style=\"margin-left:5px\" type=\"warning\"@click=\"reset\"><i class=\"el-icon-refresh\"></i> 重置</el-button>\r\n        </div>\r\n\r\n\r\n        <el-table :data=\"tableData\" stripe>\r\n            <el-table-column prop=\"id\" label=\"编号\" width=\"50\"></el-table-column>\r\n            <el-table-column prop=\"username\" label=\"用户名\"></el-table-column>\r\n<!--            <el-table-column prop=\"cardID\" label=\"卡号\"></el-table-column>-->\r\n            <el-table-column prop=\"phone\" label=\"联系方式\"></el-table-column>\r\n            <el-table-column prop=\"createtime\" label=\"创建时间\"></el-table-column>\r\n            <el-table-column prop=\"updatetime\" label=\"更新时间\"></el-table-column>\r\n            <el-table-column prop=\"email\" label=\"邮箱\"></el-table-column>\r\n\r\n            <el-table-column  label=\"状态\" width=\"230px\">\r\n                <template v-slot=\"scope\">\r\n                    <el-switch\r\n                            v-model=\"scope.row.status\"\r\n                            @change=\"changeStatus\"\r\n                            active-color=\"#13ce66\"\r\n                            inactive-color=\"#ff4949\">\r\n                    </el-switch>\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column  label=\"操作\" width=\"230px\">\r\n                <template v-slot=\"scope\">\r\n<!--                    scope.row 就是当前行数据-->\r\n                    <div style=\"display: flex\">\r\n                    <el-button  type=\"primary\" @click=\"$router.push('/editAdmin?id='+scope.row.id)\">编辑</el-button>\r\n                    <el-popconfirm\r\n                            style=\"margin-left: 14px;\"\r\n                            confirm-button-text='YES'\r\n                            cancel-button-text='NO'\r\n                            icon=\"el-icon-info\"\r\n                            title=\"是否确定删除该行数据\"\r\n                            @confirm=\"del(scope.row.id)\"\r\n                    >\r\n                    <el-button  type=\"danger\" slot=\"reference\">删除</el-button>\r\n                    </el-popconfirm>\r\n                    <el-button style=\"margin-left: 15px\"  type=\"warning\" @click=\"handleChangePass(scope.row)\">修改密码</el-button>\r\n                    </div>\r\n                </template>\r\n            </el-table-column>\r\n\r\n        </el-table>\r\n        <!--      分页-->\r\n        <div style=\"margin-top: 20px\">\r\n            <el-pagination\r\n                    background\r\n                    :current=\"params.pageNumber\"\r\n                    :page-size=\"params.pageSize\"\r\n                    layout=\"prev, pager, next\"\r\n                    @current-change=\"handleCurrentChanged\"\r\n                    :total=\"total\">\r\n            </el-pagination>\r\n        </div>\r\n\r\n        <el-dialog title=\"修改密码\" :visible.sync=\"dialogFormVisible\" width=\"30%\">\r\n            <el-form :model=\"form\" label-width=\"100px\" :rules=\"rules\" ref=\"formRef\" >\r\n                <el-form-item label=\"新密码\" prop=\"newPass\">\r\n                    <el-input v-model=\"form.newPass\" autocomplete=\"off\" show-password></el-input>\r\n                </el-form-item>\r\n\r\n            </el-form>\r\n            <div slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"savePass\">确 定</el-button>\r\n            </div>\r\n        </el-dialog>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport request from \"@/utils/request\";\r\nimport Cookies from 'js-cookie';\r\nexport default {\r\n    name: 'AdminList',\r\n    data(){\r\n        return {\r\n            admin: Cookies.get('admin') ? JSON.parse(Cookies.get('admin')) : {},\r\n            tableData: [],\r\n            total:0,\r\n            form:{},\r\n            dialogFormVisible:false,\r\n            params:{\r\n                pageNumber:1,\r\n                pageSize:10,\r\n                username:'',\r\n                phone:'',\r\n                email:''\r\n            },\r\n            rules:{\r\n                newPass: [\r\n                    { required: true, message: '请输入新密码', trigger: 'blur' },\r\n                    {min:3,max:10,message: '长度在3-10个字符',trigger:'blur'}\r\n                ],\r\n            }\r\n        }\r\n    },\r\n    created() {\r\n        this.load()\r\n    },\r\n    methods:{\r\n        load(){\r\n            // fetch('http://localhost:9090/admin/list').then(res=>res.json()).then(res=>{\r\n            //     console.log(res)\r\n            //     this.tableData=res\r\n            // })\r\n            request.get('/admin/page',{\r\n                params: this.params\r\n            }).then(res=>{\r\n                if(res.code=='200') {\r\n                    this.tableData = res.data.list\r\n                    this.total = res.data.total\r\n                }\r\n\r\n            })\r\n        },\r\n        reset(){\r\n            this.params = {\r\n                pageNumber:1,\r\n                pageSize:10,\r\n                username:'',\r\n                phone:'',\r\n                email: ''\r\n            }\r\n            this.load()\r\n        },\r\n        del(id){\r\n            request.delete(\"/admin/delete/\"+id).then(res=>{\r\n                if(res.code=='200') {\r\n                    this.$notify.success(\"删除成功\")\r\n                    this.load()\r\n                }\r\n                else\r\n                    this.$notify.error(res.msg)\r\n            });\r\n\r\n        },\r\n        //点击切换分页\r\n        handleCurrentChanged(pageNumber){\r\n            this.params.pageNumber=pageNumber\r\n            this.load()\r\n        },\r\n        handleChangePass(row){\r\n            this.dialogFormVisible=true\r\n            this.form=JSON.parse(JSON.stringify(row))\r\n        },\r\n        savePass(){\r\n            this.$refs[\"formRef\"].validate((valid) => {\r\n                if (valid) {\r\n                  request.put('/admin/password',this.form).then(res=>{\r\n                      if(res.code === '200') {\r\n                          this.$notify.success(\"修改成功\")\r\n                          if (this.form.id === this.admin.id) { //当前修改的用户id等于当前登录的管理员id，修改成功后需要重新登录\r\n                              Cookies.remove('admin')\r\n                              this.$router.push('/login')\r\n                          }else{\r\n                              this.load()\r\n                              this.dialogFormVisible=false\r\n                          }\r\n                      }else{\r\n                          this.$notify.error(\"修改失败\")\r\n                      }\r\n                    })\r\n                }\r\n            })\r\n\r\n        },\r\n    }\r\n}\r\n</script>\r\n"],"mappings":";AAgFA,OAAAA,OAAA;AACA,OAAAC,OAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,KAAA,EAAAH,OAAA,CAAAI,GAAA,YAAAC,IAAA,CAAAC,KAAA,CAAAN,OAAA,CAAAI,GAAA;MACAG,SAAA;MACAC,KAAA;MACAC,IAAA;MACAC,iBAAA;MACAC,MAAA;QACAC,UAAA;QACAC,QAAA;QACAC,QAAA;QACAC,KAAA;QACAC,KAAA;MACA;MACAC,KAAA;QACAC,OAAA,GACA;UAAAC,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,GACA;UAAAC,GAAA;UAAAC,GAAA;UAAAH,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EACAG,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAD,KAAA;MACA;MACA;MACA;MACA;MACA1B,OAAA,CAAAK,GAAA;QACAO,MAAA,OAAAA;MACA,GAAAgB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAtB,SAAA,GAAAqB,GAAA,CAAA1B,IAAA,CAAA4B,IAAA;UACA,KAAAtB,KAAA,GAAAoB,GAAA,CAAA1B,IAAA,CAAAM,KAAA;QACA;MAEA;IACA;IACAuB,MAAA;MACA,KAAApB,MAAA;QACAC,UAAA;QACAC,QAAA;QACAC,QAAA;QACAC,KAAA;QACAC,KAAA;MACA;MACA,KAAAS,IAAA;IACA;IACAO,IAAAC,EAAA;MACAlC,OAAA,CAAAmC,MAAA,oBAAAD,EAAA,EAAAN,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAM,OAAA,CAAAC,OAAA;UACA,KAAAX,IAAA;QACA,OAEA,KAAAU,OAAA,CAAAE,KAAA,CAAAT,GAAA,CAAAU,GAAA;MACA;IAEA;IACA;IACAC,qBAAA3B,UAAA;MACA,KAAAD,MAAA,CAAAC,UAAA,GAAAA,UAAA;MACA,KAAAa,IAAA;IACA;IACAe,iBAAAC,GAAA;MACA,KAAA/B,iBAAA;MACA,KAAAD,IAAA,GAAAJ,IAAA,CAAAC,KAAA,CAAAD,IAAA,CAAAqC,SAAA,CAAAD,GAAA;IACA;IACAE,SAAA;MACA,KAAAC,KAAA,YAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA/C,OAAA,CAAAgD,GAAA,yBAAAtC,IAAA,EAAAkB,IAAA,CAAAC,GAAA;YACA,IAAAA,GAAA,CAAAC,IAAA;cACA,KAAAM,OAAA,CAAAC,OAAA;cACA,SAAA3B,IAAA,CAAAwB,EAAA,UAAA9B,KAAA,CAAA8B,EAAA;gBAAA;gBACAjC,OAAA,CAAAgD,MAAA;gBAC