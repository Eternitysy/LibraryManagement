                     cancel-button-text='NO'\r\n                            icon=\"el-icon-info\"\r\n                            title=\"是否确定删除该行数据\"\r\n                            @confirm=\"del(scope.row.id)\"\r\n                    >\r\n                    <el-button  type=\"danger\" slot=\"reference\">删除</el-button>\r\n                    </el-popconfirm>\r\n                    </div>\r\n                </template>\r\n            </el-table-column>\r\n\r\n        </el-table>\r\n        <!--      分页-->\r\n        <div style=\"margin-top: 20px\">\r\n            <el-pagination\r\n                    background\r\n                    :current=\"params.pageNumber\"\r\n                    :page-size=\"params.pageSize\"\r\n                    layout=\"prev, pager, next\"\r\n                    @current-change=\"handleCurrentChanged\"\r\n                    :total=\"total\">\r\n            </el-pagination>\r\n        </div>\r\n        <el-dialog title=\"添加二级分类\" :visible.sync=\"dialogFormVisible\" width=\"30%\">\r\n            <el-form :model=\"form\" label-width=\"100px\" style=\"width: 85%\" >\r\n                <el-form-item label=\"分类名称\" prop=\"name\">\r\n                    <el-input v-model=\"form.name\" autocomplete=\"off\" ></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"分类备注\" prop=\"remark\">\r\n                    <el-input v-model=\"form.remark\" autocomplete=\"off\" ></el-input>\r\n                </el-form-item>\r\n\r\n            </el-form>\r\n            <div slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n            </div>\r\n        </el-dialog>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport request from \"@/utils/request\";\r\nimport Cookies from 'js-cookie';\r\nexport default {\r\n    name: 'CategoryList',\r\n    data(){\r\n        return {\r\n            category: Cookies.get('category') ? JSON.parse(Cookies.get('category')) : {},\r\n            tableData: [],\r\n            total:0,\r\n            dialogFormVisible:false,\r\n            form:{},\r\n            pid:null,\r\n            params:{\r\n                pageNumber:1,\r\n                pageSize:10,\r\n                name:'',\r\n                remark:''\r\n            },\r\n\r\n        }\r\n    },\r\n    created() {\r\n        this.load()\r\n    },\r\n    methods:{\r\n        load(){\r\n            // fetch('http://localhost:9090/category/list').then(res=>res.json()).then(res=>{\r\n            //     console.log(res)\r\n            //     this.tableData=res\r\n            // })\r\n            request.get('/category/page',{\r\n                params: this.params\r\n            }).then(res=>{\r\n                if(res.code=='200') {\r\n                    this.tableData = res.data.list\r\n                    this.total = res.data.total\r\n                }\r\n\r\n            })\r\n        },\r\n        reset(){\r\n            this.params = {\r\n                pageNumber:1,\r\n                pageSize:10,\r\n                name:'',\r\n                remark: ''\r\n            }\r\n            this.load()\r\n        },\r\n        del(id){\r\n            request.delete(\"/category/delete/\"+id).then(res=>{\r\n                if(res.code=='200') {\r\n                    this.$notify.success(\"删除成功\")\r\n                    this.load()\r\n                }\r\n                else\r\n                    this.$notify.error(res.msg)\r\n            });\r\n\r\n        },\r\n        //点击切换分页\r\n        handleCurrentChanged(pageNumber){\r\n            this.params.pageNumber=pageNumber\r\n            this.load()\r\n        },\r\n        handleAdd(row){\r\n            //将当前行的id作为二级分类的pid\r\n            this.pid=row.id\r\n            this.dialogFormVisible=true\r\n        },\r\n        save(){\r\n            request.post('/category/save', this.form).then(res => {\r\n                if (res.code == '200') {\r\n                    this.$notify.success('新增二级分类成功')\r\n                    this.dialogFormVisible=false\r\n                    this.load()\r\n                } else {\r\n                    this.$notify.error(res.msg)\r\n                }\r\n\r\n            })\r\n        }\r\n        \r\n    }\r\n}\r\n</script>\r\n"],"mappings":"AAqEA,OAAAA,OAAA;AACA,OAAAC,OAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,QAAA,EAAAH,OAAA,CAAAI,GAAA,eAAAC,IAAA,CAAAC,KAAA,CAAAN,OAAA,CAAAI,GAAA;MACAG,SAAA;MACAC,KAAA;MACAC,iBAAA;MACAC,IAAA;MACAC,GAAA;MACAC,MAAA;QACAC,UAAA;QACAC,QAAA;QACAb,IAAA;QACAc,MAAA;MACA;IAEA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAD,KAAA;MACA;MACA;MACA;MACA;MACAlB,OAAA,CAAAK,GAAA;QACAQ,MAAA,OAAAA;MACA,GAAAO,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAd,SAAA,GAAAa,GAAA,CAAAlB,IAAA,CAAAoB,IAAA;UACA,KAAAd,KAAA,GAAAY,GAAA,CAAAlB,IAAA,CAAAM,KAAA;QACA;MAEA;IACA;IACAe,MAAA;MACA,KAAAX,MAAA;QACAC,UAAA;QACAC,QAAA;QACAb,IAAA;QACAc,MAAA;MACA;MACA,KAAAE,IAAA;IACA;IACAO,IAAAC,EAAA;MACA1B,OAAA,CAAA2B,MAAA,uBAAAD,EAAA,EAAAN,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAM,OAAA,CAAAC,OAAA;UACA,KAAAX,IAAA;QACA,OAEA,KAAAU,OAAA,CAAAE,KAAA,CAAAT,GAAA,CAAAU,GAAA;MACA;IAEA;IACA;IACAC,qBAAAlB,UAAA;MACA,KAAAD,MAAA,CAAAC,UAAA,GAAAA,UAAA;MACA,KAAAI,IAAA;IACA;IACAe,UAAAC,GAAA;MACA;MACA,KAAAtB,GAAA,GAAAsB,GAAA,CAAAR,EAAA;MACA,KAAAhB,iBAAA;IACA;IACAyB,KAAA;MACAnC,OAAA,CAAAoC,IAAA,wBAAAzB,IAAA,EAAAS,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAM,OAAA,CAAAC,OAAA;UACA,KAAAnB,iBAAA;UACA,KAAAQ,IAAA;QACA;UACA,KAAAU,OAAA,CAAAE,KAAA,CAAAT,GAAA,CAAAU,GAAA;QACA;MAEA;IACA;EAEA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}